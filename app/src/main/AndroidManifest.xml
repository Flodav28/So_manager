        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="fr.eseo.dis.dauvillier.so_manager.LogActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="fr.eseo.dis.dauvillier.so_manager.ProjectsActivity" />
        <activity android:name="fr.eseo.dis.dauvillier.so_manager.MasterActivity" />
        <activity android:name="fr.eseo.dis.dauvillier.so_manager.MainActivity" />
        <activity android:name="fr.eseo.dis.dauvillier.so_manager.JuryActivity" />
        <activity android:name="fr.eseo.dis.dauvillier.so_manager.ProjectDetailsActivity"></activity>
        <activity android:name="fr.eseo.dis.dauvillier.so_manager.JuryDetailsActivity"></activity>
    </application>

</manifest>